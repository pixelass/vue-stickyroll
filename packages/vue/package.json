{
  "name": "@stickyroll/vue",
  "version": "1.0.0",
  "description": "Vue.js version of stickyroll",
  "keywords": [
    "vue.js",
    "vue",
    "sticky",
    "parallax",
    "fullscreen",
    "scroll",
    "animate"
  ],
  "author": "Gregor Adams <greg@pixelass.com>",
  "homepage": "https://stickyroll.github.io/react-stickyroll/",
  "license": "MIT",
  "files": [
    "dist",
    "index.js",
    "index.css"
  ],
  "main": "index.js",
  "module": "dist/esm/index.js",
  "directories": {
    "lib": "dist",
    "doc": "doc",
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": "git@github.com:stickyroll/vue-stickyroll.git",
  "scripts": {
