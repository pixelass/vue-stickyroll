{
    "presets": [
        "@babel/env"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/proposal-class-properties",
            {
                "loose": true
            }
        ],
        "@babel/proposal-object-rest-spread",
        "babel-plugin-transform-vue-jsx",
        "@babel/plugin-transform-runtime"
    ]
}
